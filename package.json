    "express": "^4.17.1",
    "oauth": "^0.9.15",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.1",
    "@types/express": "^4.17.2",
    "@types/oauth": "^0.9.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
