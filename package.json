{
  "name": "jupiter-integration",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "workspaces": [
    "core",
    "apps",
    "message-engine",
    "dynamic-ui"
  ],
  "scripts": {
    "build:server": "./node_modules/.bin/tsc -p .",
    "build:client": "sh ./scripts/build_client.sh",
    "start": "node build/app.js",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  }
}
